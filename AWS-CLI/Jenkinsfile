 pipeline {
   agent any
    stages {
			stage('Executing the script in develop branch.') {
                 when {
                   branch 'develop'
               }
               steps {
                script {
                  		sh """                                           

                                aws configure set default.region us-east-1

                                ls -lrt ${workspace}

                                aws s3 cp --recursive --sse AES256 ${workspace} s3://chiran
                               """
                        }

	       }
			}	
    }	
	}
